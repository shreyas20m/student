<% @student_details.each do |student| %>
  <tr>
    <td><%= student.usn %></td>
    <td><%= student.name %></td>
    <td><%= link_to 'Show', student %></td>
    <td><%= link_to 'Edit', edit_student_detail_path(student) %></td>
